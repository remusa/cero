type User {
    id: ID! @id @unique
    name: String!
    email: String! @unique
    password: String!
    resetToken: String
    resetTokenExpiry: Float
    updatedAt: DateTime! @updatedAt
    createdAt: DateTime! @createdAt
}

type Fast {
    id: ID! @id @unique
    startDate: DateTime!
    endDate: DateTime
    isActive: Boolean @default(value: true)
    user: User!
    updatedAt: DateTime! @updatedAt
    createdAt: DateTime! @createdAt
}
