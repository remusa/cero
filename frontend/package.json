{
    "name": "frontend",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "start": "cross-env NODE_ENV=production react-scripts start",
        "build": "react-scripts build",
        "test2": "react-scripts test",
        "eject": "react-scripts eject",
        "lint": "eslint .",
        "lint:fix": "eslint . --fix",
        "dev": "cross-env NODE_ENV=development react-scripts start",
        "test": "cross-env NODE_ENV=test npm run test:watch",
        "test:watch": "cross-env NODE_ENV=test jest --silent --watch",
        "test:coverage": "cross-env NODE_ENV=test jest --silent --coverage",
        "deploy": "npm run build && npm run netlify:deploy",
        "netlify:deploy": "netlify deploy --dir=./build -p -m \"$(git log -1 --pretty=%B)\"",
        "backend": "cd .. && cd backend/ && npm run dev"
    },
    "dependencies": {
        "@apollo/react-hooks": "0.1.0-beta.10",
        "@babel/runtime": "^7.6.2",
        "apollo-boost": "^0.4.4",
        "apollo-client": "^2.6.4",
        "chart.js": "^2.8.0",
        "date-fns": "^2.4.1",
        "dotenv": "^8.1.0",
        "formik": "^2.0.1-rc.8",
        "graphql": "^14.5.8",
        "graphql-tag": "^2.10.1",
        "normalize.css": "^8.0.1",
        "nprogress": "^0.2.0",
        "prop-types": "^15.7.2",
        "query-string": "^6.8.3",
        "react": "^16.8.6",
        "react-apollo": "^3.0.0-beta.3",
        "react-chartjs-2": "^2.8.0",
        "react-datepicker": "^2.9.6",
        "react-dom": "^16.8.6",
        "react-onclickoutside": "^6.9.0",
        "react-particles-js": "^2.7.0",
        "react-router-dom": "^5.1.2",
        "react-scripts": "3.0.1",
        "react-spinners": "^0.5.5",
        "react-spring": "^8.0.27",
        "react-stripe-checkout": "^2.6.3",
        "react-toastify": "^5.3.1",
        "styled-components": "^5.0.0-beta.6-ej4",
        "yup": "^0.27.0"
    },
    "devDependencies": {
        "@babel/plugin-syntax-dynamic-import": "^7.2.0",
        "@babel/plugin-transform-runtime": "^7.6.2",
        "@testing-library/react": "^9.2.0",
        "cross-env": "^6.0.2",
        "eslint": "^6.5.1",
        "eslint-config-airbnb": "^18.0.1",
        "eslint-config-prettier": "^6.3.0",
        "eslint-config-wesbos": "0.0.19",
        "eslint-plugin-html": "^6.0.0",
        "eslint-plugin-import": "^2.18.2",
        "eslint-plugin-jsx-a11y": "^6.2.3",
        "eslint-plugin-prettier": "^3.1.1",
        "eslint-plugin-react": "^7.15.1",
        "eslint-plugin-react-hooks": "^2.1.1",
        "jest": "^24.9.0",
        "jest-transform-graphql": "^2.1.0",
        "netlify-cli": "^2.15.0",
        "node-sass": "^4.12.0",
        "prettier": "^1.18.2"
    },
    "eslintConfig": {
        "extends": [
            "wesbos"
        ]
    },
    "//": {
        "setupFilesAfterEnv": [
            "<rootDir>/src/jest.setup.js"
        ],
        "verbose": true
    },
    "jest": {
        "testEnvironment": "jsdom",
        "testPathIgnorePatterns": [
            "<rootDir>/node_modules/",
            "<rootDir>/build/",
            "<rootDir>/dist/"
        ],
        "transform": {
            "\\.(gql|graphql)$": "jest-transform-graphql",
            ".*": "babel-jest"
        }
    },
    "babel": {
        "presets": [
            "@babel/preset-env",
            "@babel/preset-react"
        ],
        "plugins": [
            "@babel/plugin-proposal-class-properties",
            "@babel/plugin-syntax-dynamic-import",
            "@babel/plugin-transform-runtime"
        ]
    },
    "browserslist": [
        ">0.2%",
        "not dead",
        "not ie <= 11",
        "not op_mini all"
    ]
}
