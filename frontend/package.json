{
    "name": "frontend",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "@apollo/react-hooks": "0.1.0-beta.7",
        "@emotion/core": "^10.0.10",
        "apollo-boost": "^0.4.3",
        "apollo-client": "^2.6.3",
        "chart.js": "^2.8.0",
        "date-fns": "^1.30.1",
        "dotenv": "^8.0.0",
        "formik": "^1.5.7",
        "graphql": "^14.3.1",
        "graphql-tag": "^2.10.1",
        "normalize.css": "^8.0.1",
        "nprogress": "^0.2.0",
        "prop-types": "^15.7.2",
        "query-string": "^6.8.1",
        "react": "^16.8.6",
        "react-apollo": "^2.5.7",
        "react-chartjs-2": "^2.7.6",
        "react-chartkick": "^0.3.1",
        "react-datepicker": "^2.7.0",
        "react-dom": "^16.8.6",
        "react-onclickoutside": "^6.8.0",
        "react-particles-js": "^2.6.0",
        "react-router-dom": "^5.0.1",
        "react-scripts": "3.0.1",
        "react-spinners": "^0.5.4",
        "react-spring": "^8.0.25",
        "react-toastify": "^5.3.1",
        "styled-components": "^4.3.2"
    },
    "devDependencies": {
        "@testing-library/react": "^8.0.1",
        "cross-env": "^5.2.0",
        "eslint": "^5.14.1",
        "eslint-config-airbnb": "^17.1.0",
        "eslint-config-prettier": "^5.0.0",
        "eslint-config-wesbos": "0.0.19",
        "eslint-plugin-html": "^5.0.3",
        "eslint-plugin-import": "^2.17.3",
        "eslint-plugin-jsx-a11y": "^6.2.1",
        "eslint-plugin-prettier": "^3.1.0",
        "eslint-plugin-react": "^7.13.0",
        "eslint-plugin-react-hooks": "^1.6.0",
        "graphql-tools": "^4.0.5",
        "jest": "^24.8.0",
        "jest-transform-graphql": "^2.1.0",
        "netlify-cli": "^2.11.23",
        "node-sass": "^4.12.0",
        "prettier": "^1.18.2"
    },
    "//": {
        "setupFilesAfterEnv": [
            "<rootDir>/src/jest.setup.js"
        ],
        "verbose": true
    },
    "jest": {
        "testPathIgnorePatterns": [
            "<rootDir>/node_modules/",
            "<rootDir>/build/",
            "<rootDir>/dist/"
        ],
        "transform": {
            "\\.(gql|graphql)$": "jest-transform-graphql",
            ".*": "babel-jest"
        }
    },
    "eslintConfig": {
        "extends": [
            "wesbos"
        ]
    },
    "babel": {
        "presets": [
            "@babel/preset-env",
            "@babel/preset-react"
        ],
        "plugins": [
            "@babel/plugin-proposal-class-properties"
        ]
    },
    "scripts": {
        "start": "cross-env NODE_ENV=production react-scripts start",
        "build": "react-scripts build",
        "test2": "react-scripts test",
        "eject": "react-scripts eject",
        "lint": "eslint .",
        "lint:fix": "eslint . --fix",
        "dev": "cross-env NODE_ENV=development react-scripts start",
        "test": "cross-env NODE_ENV=test npm run test:watch",
        "test:watch": "cross-env NODE_ENV=test jest --silent --watch",
        "test:coverage": "cross-env NODE_ENV=test jest --silent --coverage",
        "deploy": "npm run build && npm run netlify:deploy",
        "netlify:deploy": "netlify deploy --dir=./build -p -m \"$(git log -1 --pretty=%B)\"",
        "backend": "cd .. && cd backend/ && npm run dev"
    },
    "browserslist": [
        ">0.2%",
        "not dead",
        "not ie <= 11",
        "not op_mini all"
    ],
    "plugins": [
        "emotion"
    ]
}
